<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/home_navigation_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="caios.android.holosche.fragment.HomeFragment"
        android:label="HomeFragment" />
    <fragment
        android:id="@+id/reservedFragment"
        android:name="caios.android.holosche.fragment.ReservedFragment"
        android:label="ReservedFragment" />
    <action
        android:id="@+id/action_global_homeFragment"
        app:destination="@id/homeFragment" />
    <action
        android:id="@+id/action_global_reservedFragment"
        app:destination="@id/reservedFragment" />
</navigation>